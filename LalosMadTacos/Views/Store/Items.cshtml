@model List<MenuItem>
@{
    ViewData["Title"] = "Items";
}

<h1>Items</h1>

<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-3">
                @item.Name
                /
                @item.Price
            </div>
        }
    </div>
</div>

